<style>
    /* Body styling with a vibrant gradient background */
    body {
        font-family: Arial, sans-serif;
        background: linear-gradient(to right, #ff9a9e, #fad0c4, #fbc2eb); /* Warm pink to soft lavender gradient */
        margin: 0;
        padding: 0;
        color: #333;
    }

    /* General container styling */
    .container {
        padding: 20px;
        max-width: 1200px;
        margin: auto;
    }

    /* Flexbox layout for cards */
    .card-container {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-around;
        gap: 20px;
        padding: 20px;
    }

    /* Card styling */
    .card {
        width: 250px;
        height: auto;
        background-color: #ffffff; /* Bright white cards */
        border-radius: 10px;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
        overflow: hidden;
        transition: transform 0.3s ease, box-shadow 0.3s ease;
        text-align: center;
    }

    /* Card hover effects */
    .card:hover {
        transform: translateY(-10px);
        box-shadow: 0 8px 16px rgba(0, 0, 0, 0.3);
    }

    /* Card image */
    .card img {
        width: 100%;
        height: 200px;
        object-fit: cover;
    }

    /* Card title */
    .card .title {
        margin: 15px 0;
        font-size: 18px;
        color: #333;
        font-weight: bold;
    }

    /* Card description */
    .card .card__description {
        padding: 10px 15px;
        font-size: 14px;
        color: #555;
        line-height: 1.6;
        text-align: left;
    }

    /* Button styling */
    .card button {
        border: none;
        outline: 0;
        padding: 10px;
        color: white;
        background-color: #f77f00; /* Vibrant orange button */
        cursor: pointer;
        width: 100%;
        font-size: 16px;
        border-radius: 0 0 10px 10px;
    }

    .card button:hover {
        opacity: 0.9;
    }

    /* Section title styling */
    h1 {
        text-align: center;
        margin: 20px 0;
        font-size: 28px;
        color: #5a189a; /* Deep purple for contrast */
    }
</style>


